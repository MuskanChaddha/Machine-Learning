import matplotlib.pyplot as plt

age=[20,28,34,45,50]
salary=[2000,4000,5000,1000,3000]
plt.scatter(age,salary)
plt.show()

mi1=min(salary)
ma1=max(salary)
b=[]
for j in range(len(salary)):
    sa=(salary[j]-mi1)/(ma1-mi1)
    b.append(sa)
print("After Normalization of Salary",b)


min =min(age)
max =max(age)

a=[]
for i in range(len(age)):
 ag=(age[i]-min)/(max-min)
 a.append(ag)
print("After Normalization of Age",a)

plt.scatter(a,b)
plt.show()
